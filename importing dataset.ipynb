   "source": [
    "#importing dataset using tmdb API\n",
    "result = requests.get('https://api.themoviedb.org/3/discover/movie?api_key=bbb26402d6e15da91ca8fddc3385d554&primary_release_year=2020')\n",
    "highest_revenue = result.json()\n",
    "highest_revenue_films = highest_revenue['results']\n",
    "columns = ['film', 'revenue','original_language']\n",
    "df_api = pd.DataFrame(columns=columns)\n",
    "\n",
    "for film in highest_revenue_films:\n",
    "    film_revenue = requests.get('https://api.themoviedb.org/3/movie/'+ str(film['id']) +'?api_key=bbb26402d6e15da91ca8fddc3385d554&language=en-US')\n",
    "    film_revenue = film_revenue.json()\n",
    "    df_api.loc[len(df_api)]=[film['title'],film_revenue['revenue'],film['original_language']]\n",
    "\n",
    "  "
   ]
